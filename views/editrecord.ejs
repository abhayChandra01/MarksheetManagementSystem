    <title>
       Edit Student Info
    </title>

    <body class="bg">
        <form action="/marksheet/updaterecord" method="post">
            <input type="hidden" value="<%=result.studentid %>" name="studentid">
        <div class="root">
        
            <div class="box">
                <div class="row">
                    <div style="padding-left: 230px;font-size: 28px;color:  #fff;font-weight: bolder;letter-spacing: 2px;font-family: Lucida Handwriting;">
                        <i style="font-size: 35px;" class="fas fa-user-graduate"></i>
                        Edit Student's Info
                    </div>

                    <div style="display: flex;justify-content: right;align-items: center;width: 200;">
                        <a style="font-weight: bold" class="btn btn-outline-light" href="/marksheet/displayrecords" role="button">Show Records</a>
    
                    </div>

                </div>    
                <div class="row">
                    <div class="col gapp">
                        
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            Student's Name
                            <i style="padding: 5px;" class="fas fa-file-signature"></i>
                        </div>

                        <input required value="<%=result.studentname %>" id="sname" name="sname" class="form-control" type="text" placeholder="Student's Name">
                        
                    </div>
                    <div class="col gapp">
                        
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            Father's Name
                            <i style="padding: 5px;" class="far fa-address-book"></i>
                        </div>

                        <input required value="<%=result.fathersname %>" id="fname" name="fname" class="form-control" type="text" placeholder="Father's Name">
                    
                    </div>
                
                </div>

                <div class="row">
                    <div class="col gapp">
                        
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            Roll Number
                            <i style="padding: 5px;" class="fas fa-user-tag"></i>
                            
                        </div>

                        <input required value="<%=result.rollno %>" id="rollno" name="rollno" class="form-control" type="text" placeholder="Roll Number">

                    </div>
                    <div class="col gapp">
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            Gender
                            <i style="padding: 5px;font-size: 15px;" class="fas fa-venus-mars"></i>
                            
                        </div>

                            <select required id="gender" name="gender" class="form-control">

                                <% if(result.gender=="Male"){ %>
                                    <option>Male</option>
                                    <option>Female</option>
                                    <option>Other</option>
                                    <option>Prefer Not To Say</option>

                                    <%} else if(result.gender=="Female"){ %>
                                        <option>Female</option>
                                        <option>Male</option>
                                        <option>Other</option>
                                        <option>Prefer Not To Say</option>

                                        <%} else if(result.gender=="Other"){ %>
                                            <option>Other</option>
                                            <option>Male</option>
                                            <option>Female</option>
                                            <option>Prefer Not To Say</option>    

                                            <%} else if(result.gender=="Prefer Not To Say"){ %>
                                                <option>Prefer Not To Say</option>
                                                <option>Male</option>
                                                <option>Female</option>
                                                <option>Other</option>

                                                <%} %>

                            </select>
                        
                    </div>
                    <div class="col gapp">
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            D.O.B
                            <i style="padding: 5px;" class="fas fa-calendar-alt"></i>
                            
                        </div>
                        <input required value="<%=result.dob %>" id="dob" name="dob" class="form-control" type="date">


                    </div>
                
                </div>
                <div class="row">
                    <div class="col gapp">
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            School's Name
                            <i style="padding: 5px;" class="fas fa-school"></i>
                            
                        </div>

                        <input required value="<%=result.schoolname %>" id="school" name="school" class="form-control" type="text" placeholder="School's Name">

                    
                    </div>

                    <div class="col gapp">
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            State
                            <i style="padding: 5px;" class="fas fa-satellite"></i>
                            
                        </div>

                        <select required id="states" name="states" class="form-control">

                            <option value="<%=result.state %>"><%=result.sn %></option>
                            
                            

                        </select>
                    
                    </div>
                    <div class="col gapp">
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            City
                            <i style="padding: 5px;" class="fas fa-city"></i>
                            
                        </div>

                        <select required value="<%=result.city %>" id="cities" name="cities" class="form-control">
                            <option value="<%=result.city %>"><%=result.cn %></option>
                            
                        </select>
                    
                    </div>
                    
                </div>
                <div class="row">

                    <div class="col gapp">
                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            Address
                            <i style="padding: 5px;" class="fas fa-map-marked"></i>
                            
                        </div> 
                        <input required value="<%=result.address %>" id="address" name="address" class="form-control" type="text" placeholder="Enter Address">
                    </div>
                    <div class="col gapp">

                        <div style="font-size: 14px;color:  #fff;font-weight: bolder;">
                            Picture
                            <i style="padding: 5px;" class="fas fa-image"></i>
                            
                        </div> 
                        
                            <img src='/images/<%=result.picture %>' width="40">
                            <a style="font-weight: bold" class="btn btn-outline-light" role="button" href="/marksheet/editphoto?sid=<%=result.studentid %>&sname=<%= result.studentname %>&picture=<%=result.picture %>">Edit Photo</a>
                        
                    </div>
  
                </div>


            </div>

        </div>
        <div class="root">

            <div class="box">

